{
  "name": "plugin-rtc",
  "version": "0.1.0",
  "description": "A Twilio-CLI plugin for real-time communication apps",
  "main": "index.js",
  "scripts": {
    "test": "mocha --recursive"
  },
  "author": "",
  "license": "Apache-2.0",
  "dependencies": {
    "@oclif/command": "^1.5.19",
    "@oclif/config": "^1.14.0",
    "@twilio-labs/serverless-api": "^1.0.3",
    "@twilio/cli-core": "^4.4.6",
    "moment": "^2.24.0"
  },
  "devDependencies": {
    "mocha": "^7.1.0"
  },
  "files": [],
  "oclif": {
    "name": "rtc",
    "commands": "./src/commands",
    "bin": "twilio",
    "devPlugins": [
      "@oclif/plugin-help"
    ],
    "topics": {
      "rtc": {
        "description": "A plugin which showcases real-time communication applications that demonstrate the Twilio platform"
      },
      "rtc:video": {
        "description": "Commands related to Video apps"
      },
      "rtc:video:list": {
        "description": "List any deployed Video apps"
      },
      "rtc:video:deploy-app": {
        "description": "Deploy an Video app"
      },
      "rtc:video:delete-app": {
        "description": "Delete a deployed Video app"
      },
      "rtc:video:refresh-app-passcode": {
        "description": "Refreshes the Video app passcode"
      }
    }
  }
}
